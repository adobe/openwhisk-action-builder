    "semver": "^7.0.0",
    "webpack": "^4.39.2",
    "yargs": "^15.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "8.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "3.6.1",
    "eslint": "6.8.0",
